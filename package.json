    "@types/jest": "^25.2.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "gh-pages": "^2.2.0",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "nodemon": "^2.0.3",
    "npm-watch": "^0.6.0",
