{
  "name": "react-email-autocomplete",
  "version": "1.3.1",
  "description": "Autocomplete React component for email fields",
  "repository": {
    "type": "git",
    "url": "https://github.com/msudgh/react-email-autocomplete.git"
  },
  "author": "Masoud Ghorbnai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msudgh/react-email-autocomplete/issues"
  },
  "main": "index.js",
  "homepage": "https://github.com/msudgh/react-email-autocomplete",
  "keywords": [
    "react-component",
    "react",
    "autocomplete",
    "email",
    "input"
  ],
  "watch": {
    "prepublish": "src/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "watch": "npm-watch",
    "deploy": "gh-pages -d build",
    "build": "rollup -c -o index.js",
    "test": "jest",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --fix --verbose | snazzy",
    "formatter": "prettier --write --config .prettierrc src/**/*.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.15",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "gh-pages": "^2.0.1",
    "jest": "^24.8.0",
    "jsdom": "^15.1.1",
    "nodemon": "^1.19.1",
    "npm-watch": "^0.6.0",
    "prettier": "^1.18.2",
    "react": "^16.4.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^16.8.6",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-uglify": "^6.0.2",
    "standard": "^13.0.2"
  },
  "peerDependencies": {
    "react": "~0.14.8 || ^16.4.0",
    "react-dom": "~0.14.8 || ^16.4.0"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.5.5",
    "snazzy": "^8.0.0"
  },
  "jest": {
    "setupFiles": [
      "./setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ],
    "parser": "babel-eslint",
    "ignore": [
      "index.js"
    ]
  }
}
